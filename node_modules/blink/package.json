{
  "name": "blink",
  "version": "0.1.32",
  "description": "Blink is a JavaScript runtime for building and deploying AI agents.",
  "type": "module",
  "bin": {
    "blink": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.js",
      "require": "./dist/api/index.cjs"
    },
    "./test": {
      "types": "./dist/test.d.ts",
      "import": "./dist/test.js",
      "require": "./dist/test.cjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs"
    },
    "./runtime/node": {
      "types": "./dist/runtime/node.d.ts",
      "import": "./dist/runtime/node.js",
      "require": "./dist/runtime/node.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.15",
    "@ai-sdk/google": "^2.0.13",
    "@ai-sdk/openai": "^2.0.28",
    "@ai-sdk/react": "^2.0.35",
    "@ai-sdk/xai": "^2.0.16",
    "@blink.so/api": "^0.0.5",
    "@clack/prompts": "^0.11.0",
    "@hono/node-server": "^1.19.3",
    "@hugodutka/gemini-cli": "^0.6.0-nightly-20250912-5",
    "@mswjs/interceptors": "^0.39.6",
    "@types/marked-terminal": "^6.1.1",
    "@whatwg-node/server": "^0.10.12",
    "ai": "^5.0.34",
    "commander": "^14.0.0",
    "eventsource-parser": "^3.0.6",
    "hono": "^4.9.8",
    "ink": "^6.2.3",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "^16.2.1",
    "marked-terminal": "^7.3.0",
    "open": "^10.2.0",
    "react": "^19.1.1",
    "react-devtools-core": "^6.1.5",
    "tsdown": "^0.14.2",
    "unique-names-generator": "^4.7.1",
    "xdg-app-paths": "^8.3.0"
  },
  "optionalDependencies": {
    "esbuild": "^0.25.9"
  },
  "dependencies": {
    "zod": "^4.1.5"
  }
}
